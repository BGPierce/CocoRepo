* file stavass.a     Assembly for Staves psect stavass_a,0,0,0,0,0 nam stavass_a* strdn(). * Up & Down version of _strass(dest, src, nbytes) */* A tad faster than strup, since less init'ing & compare to 0,s.* Move array DOWN, so work from bottom up*/* strdn(dst, src, nby) ttl strdnstrdn: pshs u* *     U==sp, X==dp* *     char *quit;  ==0,S* *     X = dst; leas -2,s ldx 6,s* *     U = src; ldu 8,s* *     quit = src + nby; ldd 8,s addd 10,s std 0,s* *     for( ; sp < quit; ) bra test* *          *dp++ = *sp++;loop ldb ,u+ stb ,x+test cmpu ,s       'quit' blo loop leas 2,s puls u,pc****************** endsect* eof stavass.a