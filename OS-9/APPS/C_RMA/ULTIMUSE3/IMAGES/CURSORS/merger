-xecho Ignore Error 216del /dd/images/cursors.imgdel oldtoolsdel newtoolsdel layoutsecho Now worry.xmerge none.i arrow.i diamond.i lmark.i  rmark.i tmark.i xout.i imark.i plus.i tie.i >oldtoolsmerge barline.i chisel.i square.i rings.i question.i movenote.i screen.i squarefull.i brush.i >newtoolsmerge plus8.i minus8.i refill.i tacet.i clear.i >layoutsmerge oldtools newtools layouts >/dd/images/cursors.imgecho Done.